{
  "agent": {
    "name": "AI Coding Agent",
    "version": "1.0.0",
    "description": "Intelligent programming assistant with LLM integration"
  },
  "llm": {
    "provider": "openai",
    "model": "gpt-4",
    "temperature": 0.7,
    "maxTokens": 4000,
    "apiKey": "${LLM_API_KEY}"
  },
  "mcp": {
    "enabled": true,
    "resources": {
      "maxSize": 10485760,
      "allowedTypes": ["file", "directory", "url"]
    },
    "prompts": {
      "enabled": true,
      "customPrompts": []
    }
  },
  "tools": {
    "file": {
      "enabled": true,
      "maxFileSize": 1048576,
      "allowedExtensions": [".js", ".ts", ".jsx", ".tsx", ".json", ".md", ".txt", ".py", ".java", ".cpp", ".c", ".go", ".rs"]
    },
    "code": {
      "enabled": true,
      "languages": ["javascript", "typescript", "python", "java", "go", "rust"],
      "frameworks": ["react", "vue", "angular", "express", "nest"]
    }
  },
  "intelligentMode": {
    "enabled": true,
    "autoPlanning": true,
    "maxSteps": 10,
    "requireConfirmation": false
  },
  "security": {
    "allowFileWrite": true,
    "allowFileDelete": false,
    "sandboxMode": false,
    "maxFilesPerOperation": 100
  },
  "logging": {
    "level": "info",
    "verbose": false,
    "logFile": null
  }
}
